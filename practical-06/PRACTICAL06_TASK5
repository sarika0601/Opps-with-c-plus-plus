#include<iostream>
using namespace std;

int main()
{
string s="hi",a="hi_iii",b="hi",c="hi*iii",d="Hi";
cout<<"String comparision btw:"<<endl;
cout<<" "<<s<<" and "<<d<<": "<<s.compare(d)<<endl;
cout<<" "<<d<<" and "<<s<<": "<<d.compare(d)<<endl;
cout<<" "<<s<<" and "<<b<<": "<<s.compare(d)<<endl;
cout<<" "<<s<<" and "<<a<<": "<<s.compare(d)<<endl;
cout<<" "<<c<<" and "<<s<<": "<<c.compare(d)<<endl;
cout<<"\nDifference in ASCII value:"<<endl;
cout<<"a and A: "<<'a'-'A'<<endl;
cout<<"a and b: "<<'a'-'b'<<endl;
}
